{
    "name": "picoss/yousign-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 YouSign bundle",
    "keywords": [
        "symfony2",
        "bundle",
        "yousign"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Romain Honel",
            "email": "r.honel@yeswehack.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Picoss\\YousignBundle\\": ""
        }
    },
    "require": {
        "php": ">=5.5.9",
        "yousign/yousign-api-client": "^2.0",
        "symfony/framework-bundle": "~2.8 || ^3.0 || ^4.0",
        "symfony/options-resolver": "~2.8 || ^3.0 || ^4.0",
        "doctrine/collections": "~1.2",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
